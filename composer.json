{
  "name": "mauijay/boilerplate",
  "description": "CodeIgniter4 Admin Boilerplate based on SB2 by jay",
  "keywords": [
      "codeigniter4",
      "authentication",
      "authorization",
      "boilerplate"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
      {
          "name": "Jay Lamping",
          "email": "jaycadla@gmail.com",
          "role": "Developer"
      }
  ],
  "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "fakerphp/faker": "^1.13"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "mauijay\\boilerplate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
      "test": "phpunit"
    }
}